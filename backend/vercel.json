{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "crons": [
    {
      "path": "/worksheet-response/auto-submit",
      "schedule": "45 7 * * *"
    }
  ]
}